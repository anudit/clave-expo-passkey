{
  "name": "clave-expo-passkey-example",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --profile development --platform android --local --output app-android.apk",
    "build:android-prod": "eas build --non-interactive --profile production --platform android --local --output app-android-prod.apk",
    "build:ios": "eas build --non-interactive --profile development --platform ios --local --output app-ios.tar.gz",
    "build:ios-prod": "eas build --non-interactive --profile preview --platform ios --local --output app-ios-prod.ipa"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "~50.0.2",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "expo-dev-client": "~3.3.12"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "^5.1.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
